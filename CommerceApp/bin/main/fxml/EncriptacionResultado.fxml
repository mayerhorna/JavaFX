<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.commerceapp.controller.EncriptacionResultadoController">
   <children>
      <Label fx:id="lblTituloEncriptadoEnLegalizacion" layoutX="95.0" layoutY="42.0" text="El fichero encriptado se ha generado correctamente en la siguiente localización" />
      <ImageView fitHeight="37.0" fitWidth="51.0" layoutX="36.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imagenes/Toolbar-IconoOk.png" />
         </image>
      </ImageView>
      <Label fx:id="lblTextoExplicativoEncriptadoManual" layoutX="57.0" layoutY="118.0" prefHeight="63.0" prefWidth="564.0" text="Para poder desencriptar el fichero, además de recordar su contraseña, deberá tener en cuenta la siguiente información" wrapText="true" />
      <Label fx:id="lblFicheroSalida" layoutX="65.0" layoutY="87.0" text="Fichero" />
      <TextArea fx:id="txtResultado" editable="false" layoutX="54.0" layoutY="181.0" prefHeight="151.0" prefWidth="571.0" />
      <Button fx:id="btnCopiarPortapaleles" layoutX="47.0" layoutY="347.0" mnemonicParsing="false" onAction="#btnCopiarPortapapeles_Click" text="Copiar información al portapapeles y cerrar" />
      <Button fx:id="btnCerrar" layoutX="587.0" layoutY="347.0" mnemonicParsing="false" onAction="#btnCerrar_Click" text="Cerrar" />
      <TextField fx:id="txtFicheroSalida" editable="false" layoutX="131.0" layoutY="82.0" prefHeight="30.0" prefWidth="463.0" />
   </children>
</AnchorPane>
