<AnchorPane fx:id="frmPrevioEnvio" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.commerceapp.controller.PrevioEnvioController">
   <children>
      <GridPane hgap="20.0" vgap="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="25.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="15.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="25.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="5.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="5.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="5.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" percentHeight="5.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" percentHeight="15.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextArea fx:id="txtAviso" editable="false" prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnSpan="7" />
            <Label fx:id="lblGridFicherosPdf" text="Label" GridPane.rowIndex="2" />
            <ListView fx:id="lsvFicheros" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="3" GridPane.rowSpan="4" />
            <Button fx:id="btnEnviar" mnemonicParsing="false" onAction="#btnEnviar_Click" text="Button" GridPane.rowIndex="7" />
            <VBox spacing="30.0" GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.rowSpan="4">
               <children>
                  <Button fx:id="btnAgregarPdf" maxHeight="2000.0" maxWidth="2000.0" mnemonicParsing="false" onAction="#btnAgregarPdf_Click" text="Button" textAlignment="CENTER" wrapText="true" VBox.vgrow="ALWAYS" />
                  <Button fx:id="btnQuitarPdf" maxHeight="2000.0" maxWidth="2000.0" mnemonicParsing="false" onAction="#btnQuitarPdf_Click" text="Button" textAlignment="CENTER" wrapText="true" VBox.vgrow="ALWAYS" />
                  <Button fx:id="btnAbrirPdf" maxHeight="2000.0" maxWidth="2000.0" mnemonicParsing="false" onAction="#btnAbrirPdf_Click" text="Button" textAlignment="CENTER" wrapText="true" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox spacing="25.0" style="-fx-border-color: silver;" GridPane.columnIndex="2" GridPane.rowIndex="3" GridPane.rowSpan="2">
               <children>
                  <RadioButton fx:id="rbtEnvioDirecto" mnemonicParsing="false" text="RadioButton" />
                  <RadioButton fx:id="rbtEnvioPortal" mnemonicParsing="false" text="RadioButton" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <HBox fx:id="grpNumeroDocumentoCaja" style="-fx-border-color: silver;" GridPane.columnIndex="2" GridPane.rowIndex="6">
               <children>
                  <TextField fx:id="txtNumeroDocumento" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <Label fx:id="grpNumeroDocumento" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="5" />
            <Label fx:id="grpEnvio" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label fx:id="grpEntradaTipo" text="Label" GridPane.columnIndex="3" GridPane.rowIndex="2" />
            <VBox fx:id="grpEntradaTipoCaja" spacing="25.0" style="-fx-border-color: silver;" GridPane.columnIndex="3" GridPane.columnSpan="3" GridPane.rowIndex="3" GridPane.rowSpan="2">
               <children>
                  <RadioButton fx:id="rbtEntradaTipoPrimera" mnemonicParsing="false" text="RadioButton" />
                  <RadioButton fx:id="rbtEntradaTipoSubsanacion" mnemonicParsing="false" text="RadioButton" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <Label fx:id="grpEntradaSubsanada" text="Label" GridPane.columnIndex="3" GridPane.rowIndex="5" />
            <HBox fx:id="grpEntradaSubsanadaCaja" spacing="20.0" style="-fx-border-color: silver;" GridPane.columnIndex="3" GridPane.columnSpan="4" GridPane.hgrow="ALWAYS" GridPane.rowIndex="6">
               <children>
                  <Label fx:id="lblEntradaSubsanadaLibro" text="Label" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtEntradaSubsanadaLibro" prefWidth="50.0" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </TextField>
                  <Label fx:id="lblEntradaSubsanadaAnyo" text="Label" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtEntradaSubsanadaAnyo" prefWidth="50.0" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </TextField>
                  <Label fx:id="lblEntradaSubsanadaNumero" text="Label" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtEntradaSubsanadaNumero" prefWidth="60.0" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </TextField>
               </children>
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#btnCancelar_Click" text="Cancelar" GridPane.columnIndex="5" GridPane.columnSpan="2" GridPane.halignment="RIGHT" GridPane.rowIndex="7" />
